CREATE SCHEMA jore3 AUTHORIZATION jore3importer;
CREATE SCHEMA internal_jore3 AUTHORIZATION jore3importer;

GRANT
  USAGE
  ON SCHEMA jore3
  TO
    jore3transformer,
    mapmatcher;

GRANT
  SELECT, REFERENCES
  ON ALL TABLES
  IN SCHEMA jore3
  TO
    jore3transformer,
    mapmatcher;

GRANT
  SELECT
  ON ALL SEQUENCES
  IN SCHEMA jore3
  TO
    jore3transformer,
    mapmatcher;

GRANT
  EXECUTE
  ON ALL ROUTINES
  IN SCHEMA jore3
  TO
    jore3transformer,
    mapmatcher;
